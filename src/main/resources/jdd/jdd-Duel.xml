<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Stagiaires -->
	<bean id="stagiaire1" class="dev.entites.Stagiaire">
		<property name="id" value="1" />
		<property name="nom" value="Abitbol" />
		<property name="prenom" value="Georges" />
	</bean>
	<bean id="stagiaire2" class="dev.entites.Stagiaire">
		<property name="id" value="2" />
		<property name="nom" value="Mansoif" />
		<property name="prenom" value="GÃ©rard" />
	</bean>
	<bean id="stagiaire3" class="dev.entites.Stagiaire">
		<property name="id" value="3" />
		<property name="nom" value="Detable" />
		<property name="prenom" value="Sylvain" />
	</bean>

	<!-- Quizz -->
	<bean id="quizz1" class="dev.entites.Quizz">
		<property name="id" value="1" />
		<property name="titre" value="Quizz 1" />
	</bean>
	<bean id="quizz2" class="dev.entites.Quizz">
		<property name="id" value="2" />
		<property name="titre" value="Quizz 2" />
	</bean>
	<bean id="quizz3" class="dev.entites.Quizz">
		<property name="id" value="3" />
		<property name="titre" value="Quizz 3" />
	</bean>

	<!-- Duels -->
	<bean id="duel1" class="dev.entites.Duel">
		<property name="id" value="1" />
		<property name="stagiaireA" ref="stagiaire1" />
		<property name="stagiaireB" ref="stagiaire2" />
		<property name="quizz" ref="quizz1" />
	</bean>
	<bean id="duel2" class="dev.entites.Duel">
		<property name="id" value="2" />
		<property name="stagiaireA" ref="stagiaire2" />
		<property name="stagiaireB" ref="stagiaire3" />
		<property name="quizz" ref="quizz2" />
	</bean>
	<bean id="duel3" class="dev.entites.Duel">
		<property name="id" value="3" />
		<property name="stagiaireA" ref="stagiaire1" />
		<property name="stagiaireB" ref="stagiaire3" />
		<property name="quizz" ref="quizz3" />
	</bean>
</beans>
